{
  "rewrites": [{ "source": "/(.*)", "destination": "index.js" }],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "Content-Type": "application/json" }
      ]
    },
    {
      "source": "/chat-app-umber-five-18.vercel.app/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "Content-Type": "application/json" }
      ]
    }
  ]
}
